#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules/@medusajs/cli/node_modules:/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules/@medusajs/node_modules:/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules:/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules/@medusajs/cli/node_modules:/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules/@medusajs/node_modules:/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules:/workspaces/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules/@medusajs/cli/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/@medusajs+cli@2.10.3_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__88d2df2e72a846132db296c9ad78cfe5/node_modules/@medusajs/cli/cli.js" "$@"
fi
